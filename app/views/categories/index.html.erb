<h1 align="center">All Categories</h1>
<hr>
<div align="center" style="min-width: 100px; margin: 0 auto;">
<% @categories.each do |category| %>
<ul class="listing">
	<div class="row" align="center" style="max-width: 500px;">
		<div class="card col-xl-10  ">
			<li class="article-title">
				 <%= link_to "#{category.name}", category_path(category) %> 
			</li>
		</div>

	</div>
</ul>

	<% end %>
	</div>